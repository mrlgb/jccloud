<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id_jc_layout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp"
    tools:ignore="ExtraText">


    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="173dp"
        android:layout_gravity="center"
        android:paddingBottom="10dp">

        <TextView
            android:id="@+id/textViewBarcode"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:text="Barcode一栏表"
            android:textSize="12sp"
            android:textStyle="normal" />

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="217dp"
        android:layout_gravity="center"
        android:paddingBottom="10dp">

        <TextView
            android:id="@+id/textViewSAMPLES01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:text="SAMPLES0一栏表"
            android:textSize="12sp"
            android:textStyle="normal" />

    </android.support.v7.widget.CardView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        <Button
            android:id="@+id/queryBarcode"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="queryBarcode"
            tools:ignore="ButtonStyle" />
        <Button
            android:id="@+id/querySamples"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="querySamples"
            tools:ignore="ButtonStyle" />
        <Button
            android:id="@+id/queryFormInfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="queryFormInfos"
            tools:ignore="ButtonStyle" />

    </LinearLayout>">


</LinearLayout>